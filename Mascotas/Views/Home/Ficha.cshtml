@model Mascota

<h2>ficha mascotas</h2>

<fieldset class="form-horizontal">

    <legend class="label">Informacion Mascota</legend>

        <div class="form-group">
            <div class="label"> Codigo </div>
            <div class="text-muted">@Html.DisplayFor(m=>m.codigo)</div>
        </div>

        <div class="form-group">
            <div class="label"> Nombre </div>
            <div class="text-muted">@Html.DisplayFor(m => m.nombre)</div>
        </div>

        <div class="form-group">
            <div class="label"> Raza </div>
            <div class="text-muted">@Html.DisplayFor(m => m.raza)</div>
        </div>

        <div class="form-group">
            <div class="label"> Propietario </div>
            <div class="text-muted">@Html.DisplayFor(m => m.propietario)</div>
        </div>

        <div class="form-group">
            <div class="label"> Edad </div>
            <div class="text-muted">@Html.DisplayFor(m => m.edad)</div>
        </div>
</fieldset>